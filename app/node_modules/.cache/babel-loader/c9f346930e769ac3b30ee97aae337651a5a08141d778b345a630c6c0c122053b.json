{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  name: 'HelloWorld',\n  props: {\n    msg: String\n  },\n  data() {\n    return {\n      users: [],\n      newUser: {\n        name: '',\n        age: null\n      }\n    };\n  },\n  methods: {\n    fetchUsers() {\n      axios.get('/api/users').then(response => {\n        this.users = response.data;\n      }).catch(error => {\n        console.error(error);\n      });\n    },\n    createUser() {\n      axios.post('/api/users', this.newUser).then(response => {\n        this.users.push(response.data);\n        this.newUser.name = '';\n        this.newUser.age = null;\n      }).catch(error => {\n        console.error(error);\n      });\n    },\n    deleteUser(userId) {\n      axios.delete(`/api/users/${userId}`).then(response => {\n        // 請求成功處理\n        console.log(\"User deleted:\", response.data);\n        // 更新用戶列表\n        this.users = this.users.filter(user => user.id !== userId);\n        this.fetchUsers();\n      }).catch(error => {\n        // 請求失敗處理\n        console.error(\"Error deleting user:\", error);\n      });\n    },\n    updateUser(userId) {\n      const user = this.users.find(user => user._id.$oid === userId);\n      const updatedUser = {\n        name: user.name,\n        age: user.age\n      };\n      axios.put(`/api/users/${userId}`, updatedUser).then(response => {\n        // 更新成功后的处理\n        console.log(\"User update:\", response.data);\n        this.fetchUsers();\n      }).catch(error => {\n        console.error(error);\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","name","props","msg","String","data","users","newUser","age","methods","fetchUsers","get","then","response","catch","error","console","createUser","post","push","deleteUser","userId","delete","log","filter","user","id","updateUser","find","_id","$oid","updatedUser","put"],"sources":["/root/Django/myproject/app/src/components/HelloWorld.vue"],"sourcesContent":["<template>\n  <div class=\"hello\">\n    <h1>{{ msg }}</h1>\n    <button @click=\"fetchUsers\">Fetch Users</button>\n    <ul>\n      <li v-for=\"user in users\" :key=\"user.id\">\n        <div>\n          <el-input v-model=\"user.name\"></el-input>\n        </div>\n        <div>\n          <el-input-number v-model=\"user.age\"></el-input-number>\n        </div>\n        <div>\n          <el-button @click=\"deleteUser(user._id.$oid)\" type=\"danger\">Delete</el-button>\n          <el-button @click=\"updateUser(user._id.$oid)\" type=\"primary\">Update</el-button>\n        </div>\n      </li>\n    </ul>\n    <form @submit.prevent=\"createUser\">\n      <input type=\"text\" v-model=\"newUser.name\" placeholder=\"Name\" required>\n      <input type=\"number\" v-model=\"newUser.age\" placeholder=\"Age\" required>\n      <button type=\"submit\">Create User</button>\n    </form>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios'\n\nexport default {\n  name: 'HelloWorld',\n  props: {\n    msg: String\n  },\n  data() {\n    return {\n      users: [],\n      newUser: {\n        name: '',\n        age: null\n      }\n    }\n  },\n  methods: {\n    fetchUsers() {\n      axios.get('/api/users')\n        .then(response => {\n          this.users = response.data\n        })\n        .catch(error => {\n          console.error(error)\n        })\n    },\n    createUser() {\n      axios.post('/api/users', this.newUser)\n        .then(response => {\n          this.users.push(response.data)\n          this.newUser.name = ''\n          this.newUser.age = null\n        })\n        .catch(error => {\n          console.error(error)\n        })\n    },\n    deleteUser(userId) {\n      axios.delete(`/api/users/${userId}`)\n        .then(response => {\n          // 請求成功處理\n          console.log(\"User deleted:\", response.data);\n          // 更新用戶列表\n          this.users = this.users.filter(user => user.id !== userId);\n          this.fetchUsers();\n        })\n        .catch(error => {\n          // 請求失敗處理\n          console.error(\"Error deleting user:\", error);\n        });\n    },\n    updateUser(userId) {\n      const user = this.users.find(user => user._id.$oid === userId);\n      const updatedUser = {\n        name: user.name,\n        age: user.age\n      };\n      axios.put(`/api/users/${userId}`, updatedUser)\n        .then(response => {\n          // 更新成功后的处理\n          console.log(\"User update:\", response.data);\n          this.fetchUsers();\n        })\n        .catch(error => {\n          console.error(error);\n        });\n    }\n  }\n}\n</script>\n\n<style scoped>\nh3 {\n  margin: 40px 0 0;\n}\n\nul {\n  list-style-type: none;\n  padding: 0;\n}\n\nli {\n  display: inline-block;\n  margin: 0 10px;\n}\n\na {\n  color: #42b983;\n}\n</style>"],"mappings":";AA2BA,OAAOA,KAAI,MAAO,OAAM;AAExB,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,KAAK,EAAE;IACLC,GAAG,EAAEC;EACP,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,OAAO,EAAE;QACPN,IAAI,EAAE,EAAE;QACRO,GAAG,EAAE;MACP;IACF;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,UAAUA,CAAA,EAAG;MACXV,KAAK,CAACW,GAAG,CAAC,YAAY,EACnBC,IAAI,CAACC,QAAO,IAAK;QAChB,IAAI,CAACP,KAAI,GAAIO,QAAQ,CAACR,IAAG;MAC3B,CAAC,EACAS,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACD,KAAK,CAACA,KAAK;MACrB,CAAC;IACL,CAAC;IACDE,UAAUA,CAAA,EAAG;MACXjB,KAAK,CAACkB,IAAI,CAAC,YAAY,EAAE,IAAI,CAACX,OAAO,EAClCK,IAAI,CAACC,QAAO,IAAK;QAChB,IAAI,CAACP,KAAK,CAACa,IAAI,CAACN,QAAQ,CAACR,IAAI;QAC7B,IAAI,CAACE,OAAO,CAACN,IAAG,GAAI,EAAC;QACrB,IAAI,CAACM,OAAO,CAACC,GAAE,GAAI,IAAG;MACxB,CAAC,EACAM,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACD,KAAK,CAACA,KAAK;MACrB,CAAC;IACL,CAAC;IACDK,UAAUA,CAACC,MAAM,EAAE;MACjBrB,KAAK,CAACsB,MAAM,CAAE,cAAaD,MAAO,EAAC,EAChCT,IAAI,CAACC,QAAO,IAAK;QAChB;QACAG,OAAO,CAACO,GAAG,CAAC,eAAe,EAAEV,QAAQ,CAACR,IAAI,CAAC;QAC3C;QACA,IAAI,CAACC,KAAI,GAAI,IAAI,CAACA,KAAK,CAACkB,MAAM,CAACC,IAAG,IAAKA,IAAI,CAACC,EAAC,KAAML,MAAM,CAAC;QAC1D,IAAI,CAACX,UAAU,CAAC,CAAC;MACnB,CAAC,EACAI,KAAK,CAACC,KAAI,IAAK;QACd;QACAC,OAAO,CAACD,KAAK,CAAC,sBAAsB,